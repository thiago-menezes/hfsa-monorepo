{
  "name": "@hfsa/openapi-ts",
  "version": "1.0.0",
  "description": "OpenAPI TypeScript mock generator for HFSA monorepo",
  "main": "dist/main.js",
  "types": "dist/types.d.ts",
  "bin": {
    "hfsa-openapi": "dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate": "node dist/main.js",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openapi",
    "typescript",
    "mock",
    "codegen"
  ],
  "author": "HFSA Team",
  "license": "MIT",
  "dependencies": {
    "glob": "^11.0.3",
    "js-yaml": "^4.1.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/glob": "^9.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
